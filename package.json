{
  "name": "fetchit",
  "version": "3.0.0-beta.2",
  "description": "Extra utilities for `fetch` that work in both Node and the browser.",
  "repository": {
    "type": "git",
    "url": "https://github.com/shnhrrsn/fetchit.git"
  },
  "license": "MIT",
  "author": "Shaun Harrison (@shnhrrsn)",
  "main": "lib/node.js",
  "browser": "lib/browser.js",
  "scripts": {
    "build": "rm -fr lib && tsc",
    "commit": "git-cz",
    "prepack": "yarn build",
    "release": "standard-version",
    "release:beta": "standard-version --prerelease beta",
    "test": "ts-node node_modules/ava/cli.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@ungap/url-search-params": "^0.1.1",
    "ava": "^3.11.1",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.2.17",
    "standard-version": "^9.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.6.3"
  },
  "optionalDependencies": {
    "node-fetch": "*"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
